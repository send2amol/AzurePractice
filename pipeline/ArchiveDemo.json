{
	"name": "ArchiveDemo",
	"properties": {
		"activities": [
			{
				"name": "Elmah Copy",
				"type": "Copy",
				"dependsOn": [],
				"policy": {
					"timeout": "7.00:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"source": {
						"type": "SqlServerSource",
						"queryTimeout": "02:00:00",
						"isolationLevel": "ReadCommitted",
						"partitionOption": "None"
					},
					"sink": {
						"type": "AzureTableSink",
						"azureTableInsertType": "merge",
						"writeBatchSize": 10000
					},
					"enableStaging": false,
					"parallelCopies": 1,
					"dataIntegrationUnits": 2,
					"translator": {
						"type": "TabularTranslator",
						"mappings": [
							{
								"source": {
									"name": "ErrorId",
									"type": "Guid",
									"physicalType": "uniqueidentifier"
								},
								"sink": {
									"name": "ErrorId"
								}
							},
							{
								"source": {
									"name": "Application",
									"type": "String",
									"physicalType": "nvarchar"
								},
								"sink": {
									"name": "Application"
								}
							},
							{
								"source": {
									"name": "Host",
									"type": "String",
									"physicalType": "nvarchar"
								},
								"sink": {
									"name": "Host"
								}
							},
							{
								"source": {
									"name": "Type",
									"type": "String",
									"physicalType": "nvarchar"
								},
								"sink": {
									"name": "Type"
								}
							},
							{
								"source": {
									"name": "Source",
									"type": "String",
									"physicalType": "nvarchar"
								},
								"sink": {
									"name": "Source"
								}
							},
							{
								"source": {
									"name": "Message",
									"type": "String",
									"physicalType": "nvarchar"
								},
								"sink": {
									"name": "Message"
								}
							},
							{
								"source": {
									"name": "User",
									"type": "String",
									"physicalType": "nvarchar"
								},
								"sink": {
									"name": "User"
								}
							},
							{
								"source": {
									"name": "TimeUtc",
									"type": "DateTime",
									"physicalType": "datetime"
								},
								"sink": {
									"name": "TimeUtc"
								}
							},
							{
								"source": {
									"name": "StatusCode",
									"type": "Int32",
									"physicalType": "int"
								},
								"sink": {
									"name": "StatusCode"
								}
							},
							{
								"source": {
									"name": "Sequence",
									"type": "Int32",
									"physicalType": "int"
								},
								"sink": {
									"name": "Sequence"
								}
							},
							{
								"source": {
									"name": "AllXml",
									"type": "String",
									"physicalType": "ntext"
								},
								"sink": {
									"name": "AllXml"
								}
							}
						],
						"typeConversion": true,
						"typeConversionSettings": {
							"allowDataTruncation": false,
							"treatBooleanAsNumber": false
						}
					}
				},
				"inputs": [
					{
						"referenceName": "ElmahTable",
						"type": "DatasetReference"
					}
				],
				"outputs": [
					{
						"referenceName": "ElmahTableStorage",
						"type": "DatasetReference"
					}
				]
			}
		],
		"annotations": []
	}
}